{
  "name": "pikapika-backend",
  "version": "1.0.0",
  "description": "Backend for Pikapika Cleaning System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js --watch . --ext js,json --ignore node_modules/",
    "populate-guidelines": "node scripts/populate-guidelines.js",
    "populate-db": "node scripts/populate-db-only.js",
    "make-public": "node scripts/make-images-public.js",
    "prisma:generate": "prisma generate",
    "prisma:push": "prisma db push",
    "prisma:seed": "prisma db seed",
    "purge:non-guidelines": "node scripts/purge-non-guidelines.js --yes",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "@prisma/client": "6.13.0",
    "axios": "^1.11.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "prisma": "^6.13.0"
  },
  "keywords": [
    "cleaning",
    "management",
    "express",
    "postgresql"
  ],
  "author": "Pikapika Team",
  "license": "MIT"
}
